<Page x:Class="ContactListApp.GUI.Views.CreateContactPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid>
        <StackPanel>
            <TextBox x:Name="FirstNameTextBox" PlaceholderText="Förnamn" />
            <TextBox x:Name="LastNameTextBox" PlaceholderText="Efternamn" />
            <TextBox x:Name="EmailTextBox" PlaceholderText="E-post" />
            <Button Content="Spara" Click="SaveButton_Click" />
        </StackPanel>
    </Grid>
</Page>

